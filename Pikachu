# pokemoncito
public class Pikachu  extends Pokemon implements IElectrico{
    public Pikachu() {
    }

    @Override
    public void atacarPlacaje() {
        System.out.println("pikachu, mi ataque placaje");

    }

    @Override
    public void atacarAranazo() {
        System.out.println("pikachu, mi ataque ArAÃ‘ASO");
    }

    @Override
    public void atacarMordisco() {
        System.out.println("pikachu, mi ataque Mordisco");
    }

    @Override
    public void atacarImpactTrueno() {
        System.out.println("pikachu, mi ataque trueno");
    }

    @Override
    public void actacarPunioTrueno() {
        System.out.println("pikachu, mi ataque ptrueno");
    }

